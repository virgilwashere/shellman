{
    "name": "shellman",
    "displayName": "shellman",
    "description": "Bash script snippet",
    "version": "4.4.0",
    "publisher": "Remisa",
    "icon": "images/icon.png",
    "license": "SEE LICENSE IN LICENSE.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/yousefvand/shellman.git"
    },
    "galleryBanner": {
        "color": "#A79E35",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.28.1"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "shellscript",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "devDependencies": {
        "cspell": "^4.0.61",
        "fs": "0.0.1-security",
        "path": "^0.12.7"
    },
    "scripts": {
        "doc": "node docgen.js",
        "clean": "rm COMMANDS.md &> /dev/null",
        "spellcheck": "./node_modules/.bin/cspell ./**/*"
    }
}
